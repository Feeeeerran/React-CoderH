unction ListItems({lista}) {\n\n    var items=lista.map((e)=> \n        <CardItem key={e.id} item={e.producto} id={e.id}/>\n    );\n\n    \n\n    // Me onviene hacer un slo row con muchos items que se ordenen segun el espacio que ocupan\n\n    return (\n        <>\n        {lista!==0\n            ?<div className=\"shopItems\">\n            {items}\n            </div>\n            : <div className=\"shopItems\" >Cargando</div>\n            \n        }\n        </>\n    )\n}","/home/ferran/Escritorio/Curso React.js/proyecto2.0/src/body/components/list-items/card-item.js",[],"/home/ferran/Escritorio/Curso React.js/proyecto2.0/src/body/content/header.js",[],"/home/ferran/Escritorio/Curso React.js/proyecto2.0/src/body/content/navbar.js",["140"],"import React from 'react'\n\nimport {CartWidet, CartWidget} from '../components/shop'\n\nimport { Link } from 'react-router-dom'\n\n\nexport default function Navbar({widget}) {\n    return (\n        <nav>\n            <CartWidget/>\n            <ul>\n                <Link to=\"/\"><li>birraStore</li></Link>\n                <Link to=\"/shop\"><li>Shop</li></Link>\n                <Link to=\"/about\"><li>About</li></Link>\n                <Link to=\"/contact\"><li>Contact</li></Link>\n            </ul>\n        </nav>\n    )\n}\n","/home/ferran/Escritorio/Curso React.js/proyecto2.0/src/body/content/content.js",["141"],"/home/ferran/Escritorio/Curso React.js/proyecto2.0/src/body/content/about.js",[],"/home/ferran/Escritorio/Curso React.js/proyecto2.0/src/body/content/contact.js",[],"/home/ferran/Escritorio/Curso React.js/proyecto2.0/src/firebaseConfig.js",[],"/home/ferran/Escritorio/Curso React.js/proyecto2.0/src/body/components/item-detail/item-detail-container.js",["142"],"/home/ferran/Escritorio/Curso React.js/proyecto2.0/src/body/components/item-detail/item-detail.js",[],"/home/ferran/Escritorio/Curso React.js/proyecto2.0/src/body/components/items-order/order-container.js",[],"/home/ferran/Escritorio/Curso React.js/proyecto2.0/src/body/components/item-detail/item-buy.js",["143","144","145","146"],"import React,{useState} from 'react'\n\nimport ModalBuy from './modal-buy'\n\n\n\nexport default function ItemBuy({item}) {\n    const [mount,setMount]=useState(0);\n    const [confirm,setConfirm]=useState(0);\n    const [total,setTotal] = useState(0);\n\n\n    function Sum() {\n        if (mount<item.stock) {setMount(mount+1);}\n    }\n\n    function Res() {\n        if (mount!=0) {setMount(mount-1);}\n    }\n\n    function Disct() {\n        var total=item.price*mount;\n        if (mount>=6 && mount<12) {\n            return Math.floor(total*0.9);\n        } else if (mount>=12 && mount<24) {\n            return Math.floor(total*0.8);\n        } else if(mount>=24) {\n            return Math.floor(total*0.6);\n        } else {\n            return total;\n        }\n    }\n\n    function Confirm() {\n        if (mount!==0) {\n            if (confirm==0) {\n                setConfirm(1);\n                setTotal(Disct());\n            } else {\n                setConfirm(0);\n            }\n        }\n    }\n\n    return(\n        <div className=\"itemBuy\">\n            <div className=\"itemBuy_buttons\">\n                <button onClick={Res}>-</button>\n                <p className={mount==0?\"red\":\"\"}>\n                    {mount}</p>\n                <button onClick={Sum}>+</button>\n            </div>\n            <p>{Disct()} $</p>\n            <button onClick={Confirm}> Añadir al carrito</button>\n            {\n                confirm!=0\n                ? <ModalBuy quitModal={Confirm} item={item} mount={mount} total={total} />\n                :\"\"\n            }\n            \n        </div>\n    )\n}","/home/ferran/Escritorio/Curso React.js/proyecto2.0/src/body/components/item-detail/modal-buy.js",[],"/home/ferran/Escritorio/Curso React.js/proyecto2.0/src/body/components/cart/cart-widget.js",["147","148"],"import React,{useState} from 'react'\n\nimport CartDetail from './cart-detail'\nimport CartIcon from './cart-icon'\n\nexport default function CartWidet() {\n    const[show,setShow]=useState(0);\n\n    function Show () {\n        if (show==1) {setShow(0)}\n        else {setShow(1)}\n    }\n\n    return (\n        <div id=\"cartWidget\">\n            <CartIcon show={Show}/>\n\n            {\n                show==0\n                ?\"\"\n                :<CartDetail show={Show}/>\n            }\n        </div>\n    )\n}","/home/ferran/Escritorio/Curso React.js/proyecto2.0/src/body/components/cart/cart-detail.js",["149"],"import React from 'react'\nimport { Link } from 'react-router-dom'\n\n\nimport CartContent from './cart-content'\n\nimport {useCartContext} from '../../../contexts/cart-context'\n\n\n\nexport default function CartDetail({show}) {\n    const total=useCartContext().totalCart;\n\n\n    return (\n        <div className=\"cartDetailContainer\">\n            <button onClick={show}>X</button>\n            <div className=\"cartDetail\">\n                <h2>My cart {total==0?`is empty`:''}</h2>\n                <CartContent/>\n                <div className=\"finalBuy\">\n                    <p>{`TOTAL  $${total}`}</p>\n                    <button onClick={show}><Link to=\"/buy\">Finalizar compra</Link></button>\n                </div>\n            </div>\n        </div>\n    )\n}","/home/ferran/Escritorio/Curso React.js/proyecto2.0/src/body/components/cart/cart-icon.js",[],"/home/ferran/Escritorio/Curso React.js/proyecto2.0/src/body/components/cart/cart-content.js",["150"],"import React from 'react'\n\nimport CartItem from './cart-item'\n\nimport {useCartContext} from '../../../contexts/cart-context'\n\n\nexport default function CartContent() {\n    const cartContent=useCartContext().cart;\n    const {deleteCartItem}=useCartContext();\n\n\n    const cartList = cartContent.map((e)=> \n        <CartItem item={e} key={e.id}/>\n    )\n    return (\n        <div className=\"cartContent\">\n            {cartList}\n        </div>\n    )\n}","/home/ferran/Escritorio/Curso React.js/proyecto2.0/src/contexts/cart-context.js",["151"],"import React,{useContext,useState} from 'react'\n\n\nexport const CartContext=React.createContext();\nexport const useCartContext = () => useContext(CartContext);\n\nexport function CartProvider({initialValue=[],children}) {\n    const [cart,setCart]=useState(initialValue);\n    const [totalCart,setTotalCart]=useState(0);\n    \n    function addCart(nombre,mount,total,id) {\n        if (cart.length===0) {\n            const newCart=[...cart,{nombre,mount,total,id}];\n            setCart(newCart);\n            setTotalCart(total);\n        } else {\n            cart.forEach((e)=> {\n                if (e.id == id) {\n                    if (window.confirm(`Ya dispones de ${e.mount} de ${e.nombre}. Quieres agregar ${mount} más al carrito?`)) {\n                        e.mount=e.mount+mount;\n                        e.total=e.total+total;\n                        setTotalCart(totalCart+total)\n                    }\n                } else {\n                    const newCart=[...cart,{nombre,mount,total,id}];\n                    setCart(newCart);\n                    setTotalCart(totalCart+total)\n                }\n            })\n        }\n    }\n\n    \n    function deleteCartItem(id) {\n        const newCart=cart.filter(e=>e.id!==id);\n        var newTotal=0;\n        newCart.forEach(e=>newTotal=newTotal+e.total);\n        \n        setCart(newCart);\n        setTotalCart(newTotal)\n    }\n        \n\n    return (\n        <CartContext.Provider value={{cart,totalCart,addCart,deleteCartItem}}>\n            {children}\n        </CartContext.Provider>\n    );\n}","/home/ferran/Escritorio/Curso React.js/proyecto2.0/src/body/components/cart/cart-item.js",[],"/home/ferran/Escritorio/Curso React.js/proyecto2.0/src/body/content/buy.js",[],"/home/ferran/Escritorio/Curso React.js/proyecto2.0/src/body/components/buy.js",["152"],"import React from 'react'\n\nimport BuyItem from './buy/buy-item'\nimport BuyForm from './buy/buy-form'\n\nexport {\n    BuyItem,\n    BuyForm\n}","/home/ferran/Escritorio/Curso React.js/proyecto2.0/src/body/components/buy/buy-form.js",["153"],"import React,{useState} from 'react'\n\nimport {firestore} from '../../../firebaseConfig'\nimport { CartWidget } from '../shop';\n\n\nexport default function BuyForm({order,total}) {\n    console.log(\"render\")\n    const[nombre,setNombre]=useState();\n    const[apellido,setApellido]=useState();\n    const[tel,setTel]=useState();\n    const[mail,setMail]=useState();\n\n    const db=firestore;\n    const orders=db.collection(\"orders\");\n    \n\n    function getNombre(e) {\n        setNombre(e.target.value)\n    }\n    function getApellido(e) {\n        setApellido(e.target.value)\n    }\n    function getTel(e) {\n        setTel(e.target.value)\n    }\n    function getMail(e) {\n        setMail(e.target.value)\n    }\n\n\n\n    function addOrder() {\n        var date=getDate();\n\n        const newOrder= {\n            buyer:{\n                name:nombre,\n                Sname:apellido,\n                tel:tel,\n                mail:mail,\n            },\n            items:order,\n            total:total,\n            date:date,\n        }\n        \n        orders.add(newOrder)\n        .then(({id}) => {\n            console.log(id)\n        })\n        .catch((err)=> alert(\"ups, algo salió mal\"))\n    }\n\n    function getDate() {\n        var time = new Date();\n        var dd = String(time.getDate()).padStart(2, '0');\n        var mm = String(time.getMonth() + 1).padStart(2, '0');\n        var yyyy = time.getFullYear();\n        var hh = String(time.getHours());\n        var mmm = String(time.getMinutes());\n        return `${dd}/${mm}/${yyyy}  ${hh}:${mmm}`\n    }\n\n    return (\n        <div className=\"buyFormContainer\">\n            <h5>Datos del comprador</h5>\n            <form>\n                <label>\n                    Tu nombre\n                    <in